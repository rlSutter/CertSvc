<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
  </configSections>
  <appSettings>
    <add key="basepath" value="C:\Inetpub\CertSvc\"/>
    <add key="dbuser" value="DBUSER"/>
    <add key="dbpass" value="DBPASSWORD"/>
    <add key="reports" value="C:\Inetpub\CertSvc\Reports"/>
     <add key="GenCertProd_debug" value="Y"/>
    <add key="SchedGenCertProd_debug" value="Y"/>
    <add key="GenCurrentCards_debug" value="Y"/>
    <add key="cruser" value="CRYSTAL_USER"/>
    <add key="crpwd" value="CRYSTAL_PASSWORD"/>
    <!--<add key="cms" value="COMPANYDW"/>-->
    <add key="cms" value="@SAP-BOBI"/>
    <!-- '_RASFolderID' stores SAP RAS Internal ID for the folder that reports to be opened in openRASReport function reside -->
    <!--<add key="GenCertProd_RASFolderID" value="181222"/> -->
    <!--RASFolderID for reports stored in 'GenCertProd' folder in RAS server -->
    <add key="GenCertProd_RASFolderID" value="10687"/>
    <!--RASFolderID for reports stored in 'GenCertProd' folder in RAS server -->
  <add key="local.hq.datafluxapp.dms.service" value="http://datafluxapp.company.local/dms/service.asmx"/>
  </appSettings>
  <connectionStrings>
    <!-- Production -->
    <add name="siebeldb" connectionString="server=DBSERVER\DBINSTANCE;uid=dbuser;pwd=dbpassword;database=siebeldb;Min Pool Size=3;Max Pool Size=5" providerName="System.Data.SqlClient"/>
    <add name="email" connectionString="server=DBSERVER\DBINSTANCE;uid=dbuser;pwd=dbpassword;database=scanner;Min Pool Size=3;Max Pool Size=5" providerName="System.Data.SqlClient"/>
    <add name="hcidb" connectionString="server=DBSERVER\DBINSTANCE;uid=dbuser;pwd=dbpassword;Min Pool Size=3;Max Pool Size=5;Connect Timeout=10;database=" providerName="System.Data.SqlClient"/>
    <add name="dms" connectionString="server=DBSERVER\DBINSTANCE;uid=dmsuser;pwd=dmspassword;Min Pool Size=3;Max Pool Size=5;Connect Timeout=10;database=DMS" providerName="System.Data.SqlClient"/>
    <add name="adodb" connectionString="Provider=SQLNCLI11; Server=DBSERVER\DBINSTANCE; Database=;User ID=dbuser;Password=dbpassword;DataTypeCompatibility=80;ApplicationIntent=ReadOnly" providerName="System.Data.OleDb"/>
    <!-- Testing -->
    <!--<add name="siebeldb" connectionString="server=DBSERVER_TEST;uid=dbuser;pwd=dbpassword;database=siebeldb;Min Pool Size=3;Max Pool Size=5" providerName="System.Data.SqlClient"/>
    <add name="email" connectionString="server=DBSERVER_TEST;uid=dbuser;pwd=dbpassword;database=scanner;Min Pool Size=3;Max Pool Size=5" providerName="System.Data.SqlClient"/>
    <add name="hcidb" connectionString="server=DBSERVER_TEST;uid=dbuser;pwd=dbpassword;Min Pool Size=3;Max Pool Size=5;Connect Timeout=10;database=" providerName="System.Data.SqlClient"/>
    <add name="dms" connectionString="server=DBSERVER_TEST;uid=dmsuser;pwd=dmspassword;Min Pool Size=3;Max Pool Size=5;Connect Timeout=10;database=DMS" providerName="System.Data.SqlClient"/>
    <add name="adodb" connectionString="Provider=SQLNCLI11; Server=DBSERVER_TEST; Database=;User ID=dbuser;Password=dbpassword;DataTypeCompatibility=80;ApplicationIntent=ReadOnly" providerName="System.Data.OleDb"/>-->
  </connectionStrings>
  <log4net>
    <appender name="RemoteSyslogAppender" type="log4net.Appender.RemoteSyslogAppender">
      <identity value=""/>
      <layout type="log4net.Layout.PatternLayout" value="%message"/>
      <!-- Production -->
      <remoteAddress value="192.168.1.200"/>
      <!-- Testing -->
      <!--<remoteAddress value="192.168.1.201"/>-->
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="DEBUG"/>
      </filter>
    </appender>
    <appender name="LogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="%property{LogFileName}"/>
      <lockingModel type="log4net.Appender.RollingFileAppender+MinimalLock"/>
      <appendToFile value="true"/>
      <rollingStyle value="Size"/>
      <maxSizeRollBackups value="3"/>
      <maximumFileSize value="10000KB"/>
      <staticLogFileName value="true"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%message%newline"/>
      </layout>
    </appender>
    <appender name="GCPLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="%property{GCPLogFileName}"/>
      <lockingModel type="log4net.Appender.RollingFileAppender+MinimalLock"/>
      <appendToFile value="true"/>
      <rollingStyle value="Size"/>
      <maxSizeRollBackups value="3"/>
      <maximumFileSize value="10000KB"/>
      <staticLogFileName value="true"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%message%newline"/>
      </layout>
    </appender>
    <appender name="SGCPLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="%property{SGCPLogFileName}"/>
      <lockingModel type="log4net.Appender.RollingFileAppender+MinimalLock"/>
      <appendToFile value="true"/>
      <rollingStyle value="Size"/>
      <maxSizeRollBackups value="3"/>
      <maximumFileSize value="10000KB"/>
      <staticLogFileName value="true"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%message%newline"/>
      </layout>
    </appender>
    <appender name="GCCLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="%property{GCCLogFileName}"/>
      <lockingModel type="log4net.Appender.RollingFileAppender+MinimalLock"/>
      <appendToFile value="true"/>
      <rollingStyle value="Size"/>
      <maxSizeRollBackups value="3"/>
      <maximumFileSize value="10000KB"/>
      <staticLogFileName value="true"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%message%newline"/>
      </layout>
    </appender>
    <logger name="EventLog">
      <level value="ALL"/>
      <appender-ref ref="RemoteSyslogAppender"/>
    </logger>
    <logger name="DebugLog">
      <level value="ALL"/>
      <appender-ref ref="LogFileAppender"/>
    </logger>
    <logger name="GCPDebugLog">
      <level value="ALL"/>
      <appender-ref ref="GCPLogFileAppender"/>
    </logger>
    <logger name="SGCPDebugLog">
      <level value="ALL"/>
      <appender-ref ref="SGCPLogFileAppender"/>
    </logger>
    <logger name="GCCDebugLog">
      <level value="ALL"/>
      <appender-ref ref="GCCLogFileAppender"/>
    </logger>
  </log4net>
